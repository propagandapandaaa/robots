{
  "workbench.colorCustomizations": {
    "activityBar.background": "#D41D00",      // McDonald's Red
    "activityBar.foreground": "#FFD500",      // McDonald's Yellow
    "activityBar.activeBorder": "#FFD500",    // Yellow highlight
    "activityBar.inactiveForeground": "#FFFFFFBB",

    "titleBar.activeBackground": "#D41D00",   // McDonald's Red
    "titleBar.activeForeground": "#FFFFFF",   // White text
    "titleBar.inactiveBackground": "#A61800", // Darker red for inactive
    "titleBar.inactiveForeground": "#FFFFFFAA",

    "editor.background": "#FFD500",           // McDonald's Yellow
    "editor.foreground": "#000000",           // Black text
    "editor.selectionBackground": "#D41D00AA",// Transparent red selection
    "editor.selectionHighlightBackground": "#D41D0033",
    "editor.lineHighlightBackground": "#D41D0022",

    "editorCursor.foreground": "#D41D00",     // Red cursor
    "editorWhitespace.foreground": "#D41D0055",

    "editorLineNumber.foreground": "#A61800", // Dark red line numbers
    "editorLineNumber.activeForeground": "#D41D00",

    "sideBar.background": "#FFD500",          // Yellow sidebar
    "sideBar.foreground": "#000000",          // Black text
    "sideBar.border": "#D41D00",
    
    "statusBar.background": "#D41D00",        // Red status bar
    "statusBar.foreground": "#FFD500",        // Yellow text
    "statusBar.border": "#A61800",

    "terminal.background": "#FFD500",         // Yellow terminal
    "terminal.foreground": "#000000",         // Black text
    "terminalCursor.foreground": "#D41D00",   // Red cursor
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#D41D00",
    "terminal.ansiGreen": "#00A020",
    "terminal.ansiYellow": "#FFD500",
    "terminal.ansiBlue": "#0055AA",
    "terminal.ansiMagenta": "#A000A0",
    "terminal.ansiCyan": "#009999",
    "terminal.ansiWhite": "#FFFFFF"
  },
  "files.associations": {
    "*.sqlbook": "sql",
    "*.ndjson": "jsonl",
    "*.dbclient-js": "javascript",
    ".env*": "dotenv",
    "iostream": "cpp"
  }
}